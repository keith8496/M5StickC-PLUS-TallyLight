        "mydb": "f3b2cb2dfae64c41",
        "sqlquery": "fixed",
        "sql": "SELECT T1.*\n  FROM deviceStatusLog T1\n WHERE 1 = 1\n   AND EXISTS (SELECT 1 FROM deviceStatusLog T2\n                WHERE T1.deviceId = T2.deviceId\n                GROUP BY T2.deviceId\n                HAVING MAX(T2.timestamp) = T1.timestamp)",
